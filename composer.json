{
    "name": "framework/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.62",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.8",
        "friendsofphp/php-cs-fixer": "~2.8"
    },
    "scripts": {
        "cs-fixer": "bin/php-cs-fixer fix --config=php_cs.dist"
    },
    "autoload": {
        "psr-4": {
            "App\\": "framework/app/",
            "App\\ApplicationName\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Apps\\Laravel\\Tests\\": "framework/tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
